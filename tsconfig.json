      "outDir": "./assets/scripts",
      "rootDir": "./src",
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules",
    ]
}